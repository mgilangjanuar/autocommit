{
  "name": "gptcommit",
  "version": "0.1.21",
  "main": "dist/index.mjs",
  "author": "M Gilang Januar",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rimraf dist && tsc --noEmit false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "package": "yarn build && cp ./package.pkg.json ./dist/package.json && cd dist && ncc build -o ./build ./index.js && sed -i '' '2s/$/\\\nimport { fileURLToPath } from \"url\";\\\nconst __filename = fileURLToPath(import.meta.url);\\\n/' build/index.js && cp ../package.pkg.json ./build/package.json && cd ./build && pkg package.json"
  },
  "bin": {
    "gptcommit": "./dist/index.mjs"
  },
  "pkg": {
    "scripts": "./dist/build/index.mjs",
    "assets": [
      "./dist/build"
    ],
    "target": [
      "node18-macos-arm64"
    ]
  },
  "preferGlobal": true,
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/configstore": "^6.0.0",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "8.2.3",
    "@types/node": "^18.14.5",
    "@types/ora": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.35.0",
    "pkg": "^5.8.0",
    "rimraf": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "commander": "^10.0.0",
    "configstore": "3.1.5",
    "figlet": "^1.5.2",
    "inquirer": "8.2.3",
    "ora": "5.4.1"
  }
}
